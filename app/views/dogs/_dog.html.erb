<div class="col-sm-6">
  <div class="card bg-light mb-3">
    <div class="card-body">
      <h5 class="card-title"><%= link_to dog.name, dog_path(dog) %></h5>
      <h6 class="card-subtitle mb-2 text-muted"><%= dog.created_at.strftime '%b %m, %Y' %></h6>
      <div  class="card-text">
        
        <%= dog.describe %>
      
        <div>
          <% dog.services_a.each do |service| %>
            <span class="badge badge-secondary"><%= service.to_s.humanize %></span>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>