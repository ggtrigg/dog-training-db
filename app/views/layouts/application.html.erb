<!DOCTYPE html>
<html>
  <head>
    <title>DogTrainingDb</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <div class="row">
        <%= render "header" %>
      </div>
      <div class="row" id="application-body">
        <%= yield %>
      </div>
      <div class="row">
        <div class="col-sm-12" id="application-footer">
          <%= render "footer" %>
        </div>
      </div>
    </div> <!-- container -->
  </body>
</html>
